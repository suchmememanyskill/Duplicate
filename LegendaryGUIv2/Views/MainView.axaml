<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LegendaryGUIv2.Views.MainView">
	<DockPanel>
		<DockPanel DockPanel.Dock="Top" Background="OrangeRed" Height="50">
			<StackPanel Orientation="Horizontal" DockPanel.Dock="Left">
				<Label VerticalAlignment="Center" Margin="10,0,0,0" FontSize="24" FontWeight="Bold">Legendary GUI</Label>
				<Label VerticalAlignment="Center" Margin="10,0,0,0" Content="{Binding GameCountText}" FontSize="20"/>
			</StackPanel>
			<StackPanel Orientation="Horizontal" DockPanel.Dock="Right" HorizontalAlignment="Right">
			</StackPanel>
		</DockPanel>
		<StackPanel>
			
		</StackPanel>
	</DockPanel>
</UserControl>